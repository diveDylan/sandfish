{
  "name": "sandfish",
  "version": "0.0.5",
  "description": "create antd table columns and form items from db",
  "main": "dist/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/diveDylan/sandfish",
